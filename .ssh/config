Host validation.linaro.org
	ServerAliveInterval 60
	IdentityFile ~/.ssh/id_rsa.linaro
	User mturquette
	Port 22

Host *.val
	ServerAliveInterval 60
	user mturquette
	ProxyCommand ssh validation.linaro.org nc -q0 `basename %h .val` %p

Host git.linaro.org
	User mike.turquette

Host omap4.omapzoom.org
